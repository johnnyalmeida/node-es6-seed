{
  "name": "nodejs-seed-project",
  "version": "2.0.0",
  "description": "Seed project created to serve as a starting point for new projects, suggesting some patterns, dependencies/pacakges and style guide.",
  "main": "src/app.js",
  "scripts": {
    "start": "pm2 start process.yml",
    "stop": "pm2 stop process.yml",
    "build": "babel -d dist/ src/ --copy-files",
    "restart": "pm2 restart process.yml",
    "dev": "nodemon --exec babel-node src/app.js",
    "test": "nyc ava",
    "lint": "eslint src test || exit 0",
    "cover": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/4alltecnologia/node-es6-seed"
  },
  "keywords": [
    "seed",
    "project",
    "4all",
    "node",
    "pm2",
    "i18n",
    "knex"
  ],
  "author": "4All Tecnologia EPP",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "cron": "^1.3.0",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-winston": "^2.5.1",
    "helmet": "^3.9.0",
    "i18n": "^0.8.3",
    "joi": "^13.4.0",
    "knex": "^0.14.6",
    "lodash": "^4.17.10",
    "moment-timezone": "^0.5.14",
    "mysql2": "^1.5.1",
    "request": "^2.87.0",
    "request-debug": "^0.2.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.8.0",
    "nyc": "^13.0.0"
  }
}
